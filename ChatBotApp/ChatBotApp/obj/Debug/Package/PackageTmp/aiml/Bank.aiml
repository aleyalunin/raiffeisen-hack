<?xml version="1.0" encoding="UTF-8"?>
<aiml>
	<category>
		<pattern>НОМЕР КАРТЫ</pattern>
		<template>
			Пожалуйста, номер вашей карты - <bot name="cardnumber"/>
		</template>
	</category>	

	<category>
		<pattern>НОМЕР КАРТЫ *</pattern>
