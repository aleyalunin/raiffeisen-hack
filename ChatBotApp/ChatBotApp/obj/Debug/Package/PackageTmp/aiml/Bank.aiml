	</category>

	<category>
		<pattern>БАЛАНС</pattern>
		<template>
			На вашем счету <bot name="balance"/>р.
		</template>
	</category>

	<category>
		<pattern>* БАЛАНС</pattern>
		<template><srai>БАЛАНС</srai></template>
	</category>

	<category>
		<pattern>БАЛАНС *</pattern>
		<template><srai>БАЛАНС</srai></template>
	</category>	

</aiml>